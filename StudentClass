public class Student {

    private String name;
    private String age;
    private String gender;
    private String homeroom;

        public Student(String name, String age, String gender, String homeroom) {
            this.name = name;
            this.age = age;
            this.gender = gender;
            this.homeroom = homeroom;
        }
    public String()
    {
        return("Name: " + name + "Age: " + age + "Gender: " + gender + "Homeroom: " + homeroom)
    }

    public String getName {
        return name;
    }

    public String getAge {
        return age;
    }

    public String getGender {
        return gender;
    }

    public String getHomeroom {
        return homeroom;
    }

}
